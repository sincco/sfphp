	"license": "MIT",
	"keywords": ["sfphp","framework","sincco"],
	"version": "2.0.3",
	"authors": [
		{
			"name": "Ivan Miranda",
			"email": "ivan.miranda@sincco.com"
		}
	],
	"require": {
		"php": "^5.3.3 || ^7.0",
		"sincco/singleton": "1.0",
		"sincco/tokenizer": "^1.0",
		"monolog/monolog": "1.20",
		"desarrolla2/cache":  "2.0",
		"league/climate": "3.2.1",
		"symfony/filesystem": "^3.4",
		"composer/composer": "^1.6",
		"twig/twig": "1.27.0"
	},
	"autoload": {
